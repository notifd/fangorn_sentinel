{
  "name": "fangorn-sentinel-grafana-plugin",
  "version": "1.0.0",
  "description": "Fangorn Sentinel on-call notification plugin for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --ci",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Notifd",
  "license": "MIT",
  "devDependencies": {
    "@grafana/data": "^10.0.0",
    "@grafana/eslint-config": "^6.0.0",
