    "@grafana/schema": "^10.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@grafana/ui": "^10.0.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.0.0",
